    "rollup": "3.29.2",
    "rollup-plugin-define": "1.0.1",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "@yoskutik/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
